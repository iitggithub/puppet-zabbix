{
  "name": "iitgpuppet-zabbix",
  "version": "1.0.0",
  "author": "The Ignorant IT Guy",
  "summary": "Installs, configures, and manages Zabbix components.",
  "license": "MIT",
  "source": "https://github.com/puppetlabs/iitggithub-zabbix",
  "project_page": "https://github.com/puppetlabs/iitggithub-zabbix",
  "issues_url": "https://github.com/puppetlabs/iitggithub-zabbix",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.13.1 < 5.0.0"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "20",
        "21",
        "22"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "6",
        "7",
        "8.1",
        "10",
        "20082"
      ]
    },
    {
      "operatingsystem": "Darwin",
      "operatingsystemrelease": [
        "14",
        "15"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    }
  ],
  "description": "Zabbix monitoring system components for Debian, Ubuntu, CentOS, RHEL, Fedora, Windows and OSX."
}
